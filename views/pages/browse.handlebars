              <div class="control">
                <input
                  class="input"
                  id="filterTargetsInput"
                  type="text"
                  placeholder="Targets"
                  autocomplete="off"
                />
              </div>
            </div>

            <div
              id="filterDescSection"
              class="is-hidden filterFieldSection field"
            >
              <div class="control">
                <input
