              <div class="control">
                <input
                  class="input"
                  id="filterNameInput"
                  type="text"
                  placeholder="Name"
                  autocomplete="off"
                />
              </div>
            </div>

            <div
              id="filterTagsSection"
              class="is-hidden filterFieldSection field"
            >
              <select id="filterTagsInput" data-placeholder="Traits" multiple>
              </select>
            </div>

            <div
              id="filterFeatPrereqSection"
              class="is-hidden filterFieldSection field"
