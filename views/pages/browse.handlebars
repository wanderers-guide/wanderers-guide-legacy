              <div class="control">
                <input
                  class="input"
                  id="filterAreaInput"
                  type="text"
                  placeholder="Area"
                  autocomplete="off"
                />
              </div>
            </div>

            <div
              id="filterRangeSection"
              class="is-hidden filterFieldSection field"
            >
              <div class="control">
                <input
                  class="input"
                  id="filterRangeInput"
                  type="text"
                  placeholder="Range"
                  autocomplete="off"
                />
              </div>
            </div>

            <div
              id="filterTargetsSection"
              class="is-hidden filterFieldSection field"
