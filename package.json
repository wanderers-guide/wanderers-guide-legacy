    "@types/lodash": "^4.14.191",
    "concurrently": "^7.2.1",
    "glob-all": "^3.3.1",
    "handlebars": "4.5.3",
    "ncp": "^2.0.0",
    "nodemon": "^2.0.2",
    "prettier": "2.8.3",
    "prisma-json-schema-generator": "^3.1.2",
    "typescript": "^4.9.4"
  }
